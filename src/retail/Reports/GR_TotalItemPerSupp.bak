	<queryString><![CDATA[select gr.kode_gudang, coalesce(st.nama_gudang,'') as site_name, coalesce(gr.kode_supp,'') as kode_supp, coalesce(sp.nama_supp,'') as nama_supplier, 
count(distinct grd.kode_item) as jml_item
from r_gr gr
inner join r_gr_detail grd on grd.no_gr=gr.no_gr
left join r_supplier sp on sp.kode_supp=gr.kode_supp
left join r_gudang st on st.kode_gudang=gr.kode_gudang
where to_char(gr.tanggal, 'yyyy-MM-dd')>=$P{tanggal1}
and to_char(gr.tanggal, 'yyyy-MM-dd')<=$P{tanggal2}
and coalesce(gr.kode_gudang,'') like $P{kode_gudang}||'%'
group by gr.kode_gudang, coalesce(gr.kode_supp,'') , coalesce(sp.nama_supp,''), coalesce(st.nama_gudang,'')
order by gr.kode_gudang]]></queryString>

